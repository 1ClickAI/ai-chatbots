SERVER_URL=http://localhost:3000          # The server's URL; default is localhost for local development
API_KEY=YOUR_API_KEY                      # Replace with a randomly generated API key (use a tool like random.org/strings)
DB_CONNECTION_STRING=                     # Add your database connection string (e.g., MongoDB, PostgreSQL)
PROXY_HOST=                               # If using a proxy, provide the host address
PROXY_PORT=                               # Specify the proxy port
PROXY_PROTOCOL=                           # Specify the protocol (e.g., http, https, socks5)
PROXY_USERNAME=                           # Provide the username for proxy authentication, if required
PROXY_PASSWORD=                           # Provide the password for proxy authentication, if required
LOG_LEVEL=warn                            # Set the logging level (e.g., error, warn, info, debug)
